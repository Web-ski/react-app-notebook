CON});\n  background-size: 50%;\n  background-position: center;\n  background-repeat: no-repeat;\n  position: absolute;\n  border-radius: 50%;\n  right: 25px;\n  top: 20px;\n`;\n\nconst DetailsTemplate = ({\n  pageType,\n  title,\n  created,\n  twitterName,\n  articleUrl,\n  content,\n  cardId,\n}) => (\n  <UserPageTemplate pageType={pageType}>\n    <StyledWrapper>\n      <StyledWrapperNote>\n        <StyledHeading big>{title}</StyledHeading>\n        <DateInfo>{created}</DateInfo>\n        <Paragraph>No {cardId}</Paragraph>\n        <Paragraph>{content}</Paragraph>\n        {pageType === 'twitters' && (\n          <StyledImage\n            src={`https://res.cloudinary.com/dthynrfew/image/twitter_name/${twitterName}.jpg`}\n          />\n        )}\n        {pageType === 'articles' && <StyledLink href={articleUrl} />}\n\n        <Button as={Link} to={`/${pageType}`} activeColor={pageType}>\n          Save/Close\n        </Button>\n      </StyledWrapperNote>\n    </StyledWrapper>\n  </UserPageTemplate>\n);\n\nDetailsTemplate.propTypes = {\n  pageType: PropTypes.string.isRequired,\n  title: PropTypes.string.isRequired,\n  created: PropTypes.string.isRequired,\n  twitterName: PropTypes.string,\n  articleUrl: PropTypes.string,\n  content: PropTypes.string.isRequired,\n  cardId: PropTypes.string.isRequired,\n};\n\nDetailsTemplate.defaultProps = {\n  twitterName: null,\n  articleUrl: null,\n};\n\nexport default DetailsTemplate;\n\n// import styled from 'styled-components';\n","D:\\PROGRAMOWANIE\\projekty\\react-app-notebook\\src\\templates\\GridTemplate.js",[],"D:\\PROGRAMOWANIE\\projekty\\react-app-notebook\\src\\theme\\GlobalStyle.js",[],"D:\\PROGRAMOWANIE\\projekty\\react-app-notebook\\src\\theme\\mainTheme.js",[],"D:\\PROGRAMOWANIE\\projekty\\react-app-notebook\\src\\components\\atoms\\Heading\\Heading.js",[],"D:\\PROGRAMOWANIE\\projekty\\react-app-notebook\\src\\components\\atoms\\Paragraph\\Paragraph.js",[],"D:\\PROGRAMOWANIE\\projekty\\react-app-notebook\\src\\compo